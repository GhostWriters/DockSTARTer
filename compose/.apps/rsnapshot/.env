RSNAPSHOT<__INSTANCE>__ENABLED='false'
RSNAPSHOT<__INSTANCE>__CONTAINER_NAME='rsnapshot<__instance>'
RSNAPSHOT<__INSTANCE>__HOSTNAME='Rsnapshot<__Instance>'
RSNAPSHOT<__INSTANCE>__NETWORK_MODE=''
RSNAPSHOT<__INSTANCE>__RESTART='unless-stopped'
RSNAPSHOT<__INSTANCE>__STORAGE_ON=''     # Blank uses DOCKER_STORAGE_ON value. Set to YES/NO to override
RSNAPSHOT<__INSTANCE>__STORAGE2_ON=''    # Blank uses DOCKER_STORAGE2_ON value. Set to YES/NO to override
RSNAPSHOT<__INSTANCE>__STORAGE3_ON=''    # Blank uses DOCKER_STORAGE3_ON value. Set to YES/NO to override
RSNAPSHOT<__INSTANCE>__STORAGE4_ON=''    # Blank uses DOCKER_STORAGE4_ON value. Set to YES/NO to override
RSNAPSHOT<__INSTANCE>__TAG='latest'
RSNAPSHOT<__INSTANCE>__VOLUME_DATADIR='~/.config/appdata'
RSNAPSHOT<__INSTANCE>__VOLUME_ROOT_SSH='/root/.ssh'
RSNAPSHOT<__INSTANCE>__VOLUME_SNAPSHOTSDIR='/mnt/rsnapshot/snapshots'
